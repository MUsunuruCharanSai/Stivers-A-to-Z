
// User function Template for Java

class Solution {
    static int evenlyDivides(int n) {
        int k=n;
        int count=0;
        while(n>0){
            int digit = n%10;
            if(n!=0 && digit!=0 && k%digit==0 ){
                count++;
            }
            n=n/10;
        }
        return count;
    }
}